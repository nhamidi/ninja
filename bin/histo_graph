







i=1
while read line  
		
		case $i in
			1 ) var1=$line
				
				i=$(($i+1));;

			2 )	 var2=`echo $line-$var1 | bc -l`
				i=$(($i+1));;

			3 )	 var3=`echo $line-$var1 | bc -l`
				i=$(($i+1));;

			4 )	 var4=`echo $line-$var1 | bc -l`
				echo $line >> $current_file/$result_file/${historic[0]}_${perte_name[$k]}_time_good_$j.txt
				i=$(($i+1));;

			5 )	 var5=`echo $line-$var1 | bc -l`
				i=$(($i+1));;

			6 )	 var5=`echo $line-$var1 | bc -l`
				i=1;;
		esac	
	done < $current_file/$result_file/resultat_bis_${perte[$k]}_${historic[0]}_$j.txt

